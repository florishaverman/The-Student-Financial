rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write:if false;
    }

    match /users/{userId}{
      allow read: if request.auth.uid==userId;
    }
    match /articles/{articleId}{
      allow read;
    }
    match /blogs/{blogId}{
      allow read;
    }
    match /articles/{articleId}/comments/{commentsId}{
      allow read;
    }
  }
}
